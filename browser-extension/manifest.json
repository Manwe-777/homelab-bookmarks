{
  "manifest_version": 3,
  "name": "Smart Bookmarks Tracker",
  "version": "1.0.0",
  "description": "Tracks page visits for smart bookmark ranking",
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "http://server.local:3100/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html"
}